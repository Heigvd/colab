{
    "name": "colab-webapp-react",
    "version": "0.0.1",
    "description": "coLAB project client application",
    "repository": "http://www.github.com/Heigvd/colab.git",
    "author": "Albasim <albasim@heig-vd.ch>",
    "license": "MIT",
    "private": true,
    "main": "../../../../target/coLAB/dist/index.js",
    "scripts": {
        "clean": "rimraf ../../../../target/coLAB/dist && rimraf ../../../../target/ts-coverage && rimraf ../../../../target/ts-test-report",
        "test": "jest",
        "lint": "eslint -c .eslintrc.js --ext .ts,.tsx ./src",
        "build": "webpack --mode=production",
        "start": "webpack serve --hot --mode development",
        "preprod": "webpack serve --hot --mode production",
        "format": "prettier --write src"
    },
    "jest": {
        "collectCoverage": true,
        "testEnvironment": "jsdom",
        "coverageDirectory": "../../../../target/ts-coverage",
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!**/node_modules/**"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "roots": [
            "<rootDir>/__tests__"
        ],
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
        "testPathIgnorePatterns": [
            "/__utils__/"
        ],
        "moduleFileExtensions": [
            "js",
            "ts",
            "tsx",
            "json",
            "node"
        ],
        "moduleNameMapper": {
            "lodash-es": "lodash"
        },
        "watchPathIgnorePatterns": [
            "<rootDir>/dist/"
        ],
        "reporters": [
            "default",
            [
                "jest-html-reporter",
                {
                    "pageTitle": "Test Report",
                    "outputPath": "../../../../target/ts-test-report/index.html"
                }
            ]
        ],
        "globals": {
            "ts-jest": {
                "babelConfig": false,
                "diagnostics": {
                    "ignoreCodes": [
                        151001
                    ]
                }
            }
        }
    },
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-react": "^7.13.13",
        "@emotion/css": "^11.1.3",
        "@emotion/react": "^11.4.1",
        "@svgr/webpack": "5.5.0",
        "@types/diff": "^5.0.0",
        "@types/lodash": "^4.14.175",
        "@types/react": "^17.0.24",
        "@types/react-color": "^3.0.5",
        "@types/react-dom": "^17.0.9",
        "@types/react-redux": "^7.1.18",
        "@types/react-router-dom": "^5.3.0",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.3.0",
        "babel-loader": "^8.1.0",
        "css-loader": "^6.3.0",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-react": "^7.26.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-webpack-plugin": "^3.1.1",
        "jest": "^27.2.3",
        "jest-html-reporter": "^3.4.1",
        "prettier": "^2.4.1",
        "prettier-plugin-organize-imports": "^2.3.4",
        "redux-devtools": "^3.6.0",
        "style-loader": "^3.3.0",
        "ts-jest": "^27.0.5",
        "ts-loader": "^9.2.6",
        "typescript": "^4.4.3",
        "webpack": "^5.55.1",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.3.0",
        "webpack-react-component-name": "5.0.3"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.1.1",
        "@fortawesome/free-regular-svg-icons": "^6.1.1",
        "@fortawesome/free-solid-svg-icons": "^6.1.1",
        "@fortawesome/react-fontawesome": "^0.1.18",
        "@jsplumb/browser-ui": "^5.0.0-RC32",
        "@jsplumb/connector-bezier": "^5.0.0-RC32",
        "@jsplumb/connector-flowchart": "^5.0.0-RC32",
        "@reduxjs/toolkit": "^1.6.1",
        "colab-rest-client": "link:../../../../../colab-client/target/generated-sources/tsClient",
        "diff": "^5.0.0",
        "emotion": "^11.0.0",
        "lodash": "^4.17.21",
        "re-resizable": "^6.9.9",
        "react": "^17.0.2",
        "react-color": "^2.19.3",
        "react-dom": "^17.0.2",
        "react-password-strength-bar": "^0.4.0",
        "react-redux": "^7.2.5",
        "react-router-dom": "^6.0.1",
        "react-select": "^5.2.1"
    }
}
