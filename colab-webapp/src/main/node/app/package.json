{
  "name": "colab-webapp-react",
  "version": "0.0.1",
  "description": "coLAB project client application",
  "repository": "http://www.github.com/Heigvd/colab.git",
  "author": "Albasim <albasim@heig-vd.ch>",
  "license": "MIT",
  "private": true,
  "main": "../../../../target/coLAB/dist/index.js",
  "scripts": {
    "clean": "rimraf ../../../../target/coLAB/dist && rimraf ../../../../target/ts-coverage && rimraf ../../../../target/ts-test-report",
    "test": "jest",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx ./src",
    "build": "webpack --mode=production",
    "start": "webpack serve --hot --mode development",
    "preprod": "webpack serve --hot --mode production",
    "format": "prettier --write src"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "jsdom",
    "coverageDirectory": "../../../../target/ts-coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "babelConfig": false,
          "diagnostics": {
            "ignoreCodes": [
              151001
            ]
          }
        }
      ]
    },
    "roots": [
      "<rootDir>/__tests__"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "testPathIgnorePatterns": [
      "/__utils__/"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "lodash-es": "lodash"
    },
    "watchPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "reporters": [
      "default",
      [
        "jest-html-reporter",
        {
          "pageTitle": "Test Report",
          "outputPath": "../../../../target/ts-test-report/index.html"
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.13.13",
    "@svgr/webpack": "6.3.1",
    "@types/diff": "^5.0.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.18",
    "@types/react-color": "^3.0.5",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-html-reporter": "^3.7.0",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "redux-devtools": "^3.6.0",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.6",
    "typescript": "4.7.x",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0",
    "webpack-react-component-name": "5.0.5"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.7",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@jsplumb/browser-ui": "^5.11.3",
    "@jsplumb/connector-bezier": "^5.11.3",
    "@jsplumb/connector-flowchart": "^5.11.3",
    "@reduxjs/toolkit": "^1.8.5",
    "colab-rest-client": "link:../../../../../colab-client/target/generated-sources/tsClient",
    "diff": "^5.0.0",
    "emotion": "^11.0.0",
    "framer-motion": "^7.6.5",
    "inter-ui": "^3.19.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-password-strength-bar": "^0.4.1",
    "react-redux": "^8.0.2",
    "react-reflex": "^4.0.9",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.4.0"
  }
}
