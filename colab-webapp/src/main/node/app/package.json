{
  "name": "colab-webapp-react",
  "version": "0.0.1",
  "description": "coLAB project client application",
  "license": "MIT",
  "homepage": "https://www.colab-project.ch",
  "bugs": "https://github.com/Heigvd/colab/issues",
  "repository": "git@github.com:heigvd/colab.git",
  "author": "Albasim <albasim@heig-vd.ch>",
  "private": true,
  "main": "../../../../target/coLAB/dist/index.js",
  "scripts": {
    "clean": "rimraf ../../../../target/coLAB/dist && rimraf ../../../../target/ts-coverage && rimraf ../../../../target/ts-test-report",
    "format": "prettier --write src",
    "test": "jest",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx ./src",
    "build": "webpack --mode=production",
    "start": "webpack serve --hot --mode=development"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "jsdom",
    "coverageDirectory": "../../../../target/ts-coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "babelConfig": false,
          "diagnostics": {
            "ignoreCodes": [
              151001
            ]
          }
        }
      ]
    },
    "roots": [
      "<rootDir>/__tests__"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "testPathIgnorePatterns": [
      "/__utils__/"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "lodash-es": "lodash"
    },
    "watchPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "reporters": [
      "default",
      [
        "jest-html-reporter",
        {
          "pageTitle": "Test Report",
          "outputPath": "../../../../target/ts-test-report/index.html"
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@svgr/webpack": "^8.1.0",
    "@types/diff": "^5.0.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.2.22",
    "@types/react-color": "^3.0.5",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.2",
    "ttf-loader": "^1.0.2",
    "typescript": "^4.9.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-react-component-name": "^5.0.5"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.6",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@jsplumb/browser-ui": "^5.13.2",
    "@jsplumb/connector-bezier": "^5.13.2",
    "@jsplumb/connector-flowchart": "^5.13.2",
    "@lexical/react": "0.11.0",
    "@reduxjs/toolkit": "^1.9.1",
    "colab-rest-client": "link:../../../../../colab-client/target/generated-sources/tsClient",
    "diff": "^5.0.0",
    "emotion": "^11.0.0",
    "file-loader": "^6.2.0",
    "framer-motion": "^8.0.2",
    "inter-ui": "^3.19.3",
    "jspdf": "^2.5.1",
    "lexical": "0.11.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-password-strength-bar": "^0.4.1",
    "react-redux": "^8.0.5",
    "react-reflex": "^4.0.9",
    "react-router-dom": "^6.6.1",
    "react-select": "^5.7.0",
    "y-websocket": "^1.5.0",
    "yjs": "^13.5.50"
  }
}
