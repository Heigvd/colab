{
    "name": "colab-webapp",
    "version": "0.0.1",
    "description": "coLAB project client application",
    "repository": "http://www.github.com/Heigvd/colab.git",
    "author": "Maxence Laurent <maxence.laurent@gmail.com>",
    "license": "MIT",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "clean": "rimraf dist && rimraf coverage && rimraf test-report",
        "test": "jest",
        "lint": "eslint -c .eslintrc.js --ext .ts,.tsx ./src",
        "build": "webpack --mode=production",
        "start": "webpack serve --hot --mode development",
        "format": "prettier --write src"
    },
    "jest": {
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!**/node_modules/**"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
        "moduleFileExtensions": [
            "js",
            "ts",
            "tsx",
            "json",
            "node"
        ],
        "moduleNameMapper": {
            "lodash-es": "lodash"
        },
        "watchPathIgnorePatterns": [
            "<rootDir>/dist/"
        ],
        "reporters": [
            "default",
            [
                "jest-html-reporter",
                {
                    "pageTitle": "Test Report",
                    "outputPath": "test-report/index.html"
                }
            ]
        ],
        "globals": {
            "ts-jest": {
                "babelConfig": false,
                "diagnostics": {
                    "ignoreCodes": [
                        151001
                    ]
                }
            }
        }
    },
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/preset-env": "^7.14.4",
        "@babel/preset-react": "^7.13.13",
        "@emotion/css": "^11.1.3",
        "@emotion/react": "^11.4.0",
        "@svgr/webpack": "5.5.0",
        "@types/diff": "^5.0.0",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.170",
        "@types/react": "^17.0.8",
        "@types/react-color": "^3.0.4",
        "@types/react-dom": "^17.0.5",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.7",
        "@types/react-select": "^4.0.17",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "babel-loader": "^8.1.0",
        "css-loader": "^5.2.6",
        "eslint": "^7.27.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest": "^27.0.4",
        "jest-html-reporter": "^3.4.1",
        "prettier": "^2.3.2",
        "prettier-plugin-organize-imports": "^2.2.0",
        "redux-devtools": "^3.6.0",
        "style-loader": "^2.0.0",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.2.2",
        "typescript": "^4.3.2",
        "webpack": "^5.38.1",
        "webpack-cli": "^4.7.0",
        "webpack-dev-server": "^3.11.2"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-regular-svg-icons": "^5.15.3",
        "@fortawesome/free-solid-svg-icons": "^5.15.3",
        "@fortawesome/react-fontawesome": "^0.1.11",
        "@reduxjs/toolkit": "^1.5.1",
        "@toast-ui/react-editor": "^2.5.3",
        "colab-rest-client": "link:../../../../../colab-client/target/generated-sources/tsClient",
        "diff": "^5.0.0",
        "emotion": "^11.0.0",
        "lodash": "^4.17.21",
        "react": "^17.0.2",
        "react-color": "^2.19.3",
        "react-dom": "^17.0.2",
        "react-password-strength-bar": "^0.3.3",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "react-select": "^4.3.1"
    }
}
